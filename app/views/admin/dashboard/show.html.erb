<h1>Admin Dashboard</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Number of Products</th>
      <th scope="col">Number of Categories</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @productCount %></td>
      <td><%= @categoryCount %></td>
    </tr>
    <tr>
      <td><%= link_to "All Products", admin_products_path %></td>
      <td><%= link_to "All Categories", admin_categories_path %></td>
    </tr>
  </tbody>
</table>